import java.util.Arrays
class Solution {
    public void moveZeroes(int[] nums) {
        int j=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i] !=0){
                nums[i]=nums[j]+nums[i]-(nums[j]=nums[i]);
                j++;
            }
        }
  System.out.println(Arrays.toString(nums))
    }
}
