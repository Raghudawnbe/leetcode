class Solution {
    public int maxProfit(int[] prices) {
        int buy=prices[0];
        int max=0;
        for(int i=0;i<prices.length-1;i++){
            if(prices[i]<buy){
                buy=prices[i];
            }
             if(prices[i+1]-buy>max){
                max=Math.max(max=prices[i+1]-buy,max);
            }


        }
        return max;
    }
}
